{
  "name": "putyourlightson/craft-clear-browser-cache",
  "description": "Clears the local browser cache once per unique visitor.",
  "version": "1.0.0",
  "type": "yii2-extension",
  "license": "mit",
  "require": {
    "php": "^8.0",
    "craftcms/cms": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "putyourlightson\\clearbrowsercache\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "putyourlightson\\clearbrowsercache\\ClearBrowserCache"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "support": {
    "docs": "https://github.com/putyourlightson/craft-clear-browser-cache",
    "source": "https://github.com/putyourlightson/craft-clear-browser-cache",
    "issues": "https://github.com/putyourlightson/craft-clear-browser-cache/issues"
  }
}
